#include<stdio.h>
int main () {
int matrix [5][5]

for(int i=0;i<5;i++)
{
      for(int j =0;j<5;j++)
    {
        printf ("%c" ,matrix [i][j]);
        printf ("/n");
    }  
}
for(i=0;i<5;i++)
    {
        for(j=0;j<5;j++)
        if(i%2=0)
        {
              printf ("%c /t"; matrix [i][j]);
              printf ("/n");
        }
   }
for(i=0;i<5;i++)
{
     for(j=0;j<5;j++)
     if (i%2!=0)
    {
         printf ("%c "; matrix [i][j]);
         printf ("/n");
    }
}
for(i=0;i<5;i++)
{
     for(j=0;j<5;j
    if(i=j)
    {
        printf ("%c"; matrix [i][j]);
        printf ("/n");
    }
for(i=0;i<5;i++)
{
     for(j=0;j<5;j++)
     if(i+j=n_1)
     {
           printf ("%c /t"; matrix [i][j]);
           printf ("/n");
